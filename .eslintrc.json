{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-new": "off",
        "quotes": [
            2,
            "double"
        ],
        "indent": [
            2,
            4
        ],
        "no-console": "off",
        "max-len": "off",
        "eol-last": "off",
        "linebreak-style": "off"
    }
}